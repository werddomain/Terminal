{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"noEmitOnError": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"target": "es5",
		"lib": [
			"es2015",
			"es5",
			"dom"
		],
		"outDir": "wwwroot/js"
	},
	"exclude": [
		"ScriptsModules",
		"node_modules",
		"wwwroot"
	],
	"compileOnSave": true
}